        <Param val="&amp;#10;from PIL import Image&amp;#10;import os, time&amp;#10;import shutil          #copy &amp; C&amp;#10;import numpy as np&amp;#10;win.mouseVisible = False&amp;#10;&amp;#10;#CHANGE DEBUG TO TRUE TO REDUCE TIMES OF EVERYTHING&amp;#10;#SEE BELOW TO ADJUST TIMES MANUALLY&amp;#10;Debug=True&amp;#10;&amp;#10;Pnum=int(float(expInfo['participant']))&amp;#10;&amp;#10;#DEBUG TIMES&amp;#10;&amp;#10;sketch_time     = 10    # 20s  #time to sketch&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
