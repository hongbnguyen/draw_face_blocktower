        <Param val="class ShapeStimCustom_p extends visual.ShapeStim {&amp;#10;  _getPixiPolygon() {&amp;#10;    super._getPixiPolygon()&amp;#10;    this._pixiPolygon_px.closeStroke = this._closeShape;&amp;#10;    return this._pixiPolygon_px;&amp;#10;  }&amp;#10;}&amp;#10;&amp;#10;polygon_vertex_p = [[0, 0]];&amp;#10;polygon_vertex_p = [];&amp;#10;polygon_list_p = [];&amp;#10;Empty_shapes_p = 100;&amp;#10;for (var i = 0, _pj_a = Empty_shapes_p; (i &lt; _pj_a); i += 1) {&amp;#10;    let i_polygon_p = new ShapeStimCustom_p({win: psychoJS.window, &amp;#10;    &quot;vertices&quot;: polygon_vertex_p, &amp;#10;    &quot;closeShape&quot;: false, &amp;#10;    &quot;lineWidth&quot;: 4, &amp;#10;    lineColor: new util.Color([-1, -1, -1])});&amp;#10;    i_polygon_p.setAutoDraw(false);&amp;#10;    polygon_list_p.push(i_polygon_p);&amp;#10;}&amp;#10;&amp;#10;CircleListList_p = [];&amp;#10;CircleList_p=[];&amp;#10;CircleCordsList_p=[];&amp;#10;&amp;#10;CircleSize_p=[0.004,0.004];&amp;#10;CircleEdges_p=99;&amp;#10;CircleOpac_p=1;&amp;#10;&amp;#10;indx_shape_p = 0;&amp;#10;Drawing_p = false;&amp;#10;FirstUndoClick_p = true;&amp;#10;Exit_p = false;&amp;#10;Actionsincldel_p = 0;&amp;#10;ActionsNotincldel_p = 0;&amp;#10;NoDels_p = 0;&amp;#10;DrawRTs_p = [];&amp;#10;DelRTs_p = [];&amp;#10;LineLengths_p = [];&amp;#10;LineDurations_p = [];&amp;#10;&amp;#10;startTime_p = globalClock.getTime();&amp;#10;Diff_p=0&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="&amp;#10;##### Prepare some variables ######&amp;#10;polygon_vertex_p = [(0,0)]&amp;#10;polygon_list_p = []&amp;#10;&amp;#10;Empty_shapes_p = 100&amp;#10;for i in range(Empty_shapes_p):&amp;#10;    polygon_list_p.append(visual.ShapeStim(win, vertices=polygon_vertex_p,closeShape=False,&amp;#10;                                         fillColor=None, lineWidth=4, lineColor='black'))&amp;#10;&amp;#10;indx_shape_p = 0      # How many lines drawn so far&amp;#10;Drawing_p = False #variable necessary to only add new lines when mouse stops being clicked, and to initialise&amp;#10;#new lines when mouse is clicked again&amp;#10;FirstUndoClick_p=True #this variable stops Undo from deleting multiple lines for as long as Undo button is clocked&amp;#10;Exit_p=False # tells the task to take a screenshot and move on, when time limit reached&amp;#10;&amp;#10;# Prep some  Measurement vars&amp;#10;Actionsincldel_p=0 #number of actions (lines drawn) including deleted ones&amp;#10;ActionsNotincldel_p=0 #number of actions (lines drawn) not including deletes&amp;#10;NoDels_p=0&amp;#10;DrawRTs_p=[]&amp;#10;DelRTs_p=[]&amp;#10;LineLengths_p=[]&amp;#10;LineDurations_p=[]&amp;#10;    &amp;#10;### GET TIMER READY ####&amp;#10;startTime_p=globalClock.getTime()" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Both" valType="str" updates="None" name="Code Type"/>
        <Param val="&amp;#10;&amp;#10;&amp;#10;######### START DRAWING TASK ##########&amp;#10;# For some reason current polygon must be worked on individually, but isnt't save back to polygon_list...&amp;#10;polygon_p = polygon_list_p[indx_shape_p]&amp;#10;### DRAW LINES #####&amp;#10;# If mouse is clicked, start/continue drawing&amp;#10;if mouse_p.isPressedIn(face_p):&amp;#10; # If mouse just clicked, start a new line (polygon), record RT&amp;#10;    if not Drawing_p:&amp;#10;        polygon_vertex_p = []&amp;#10;        Drawing_p = True #Prepare to recognise when mouse is not clicked&amp;#10;        DrawRTs_p.append(globalClock.getTime()-startTime_p) #record RTs&amp;#10;        LineStartTime_p=globalClock.getTime()&amp;#10;&amp;#10;     # Update current polygon, with current mouse position&amp;#10;    polygon_vertex_p.append(mouse_p.getPos())&amp;#10;    polygon_p.vertices = polygon_vertex_p&amp;#10;elif Drawing_p: #Once mouse stops being clicked, move on to next polygon&amp;#10;        Drawing_p = False&amp;#10;        LineLengths_p.append(len(polygon_p.vertices))&amp;#10;        LineDurations_p.append(globalClock.getTime()-LineStartTime_p)&amp;#10;        indx_shape_p += 1 #move on to a new shape&amp;#10;        Actionsincldel_p +=1 #increase number of actions&amp;#10;        ActionsNotincldel_p +=1 #increase number of actions&amp;#10;&amp;#10;&amp;#10;    ### Undo Button #####&amp;#10;if mouse_p.isPressedIn(UndoButton) and FirstUndoClick_p and indx_shape_p&gt;0: #So one click at a time will remove one shape&amp;#10;    UndoButton.fillColor='white'&amp;#10;    indx_shape_p -= 1 #go back to previous shape&amp;#10;    polygon_p = polygon_list_p[indx_shape_p]&amp;#10;    polygon_p.vertices = [(0,0)] #Set current polygon back to previous one, and return to an empty set&amp;#10;    FirstUndoClick_p=False&amp;#10;    NoDels_p+=1  #increase number of deletes&amp;#10;    DelRTs_p.append(globalClock.getTime()-startTime_p)#record RTs&amp;#10;    ActionsNotincldel_p -=1 #decrease number of actions&amp;#10;elif not mouse_p.isPressedIn(UndoButton):&amp;#10;    UndoButton.fillColor='darkgray'&amp;#10;    FirstUndoClick_p=True&amp;#10;&amp;#10;&amp;#10;### Update Screen with lines #####&amp;#10;#Draw all images, and prep back image so screenshot works&amp;#10;face_p.draw()&amp;#10;# Draw every polygon done so far&amp;#10;for i_pol in range(indx_shape_p+1): #for all drawn shapes including current one&amp;#10;    polygon_list_p[i_pol].setAutoDraw(True)&amp;#10;    polygon_list_p[i_pol].draw()&amp;#10;&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="polygon_p = polygon_list_p[indx_shape_p];&amp;#10;if (mouse_p.isPressedIn(face_p)) {&amp;#10;    if ((! Drawing_p)) {&amp;#10;        polygon_vertex_p = [];&amp;#10;        Drawing_p = true;&amp;#10;        DrawRTs_p.push((globalClock.getTime() - startTime_p));&amp;#10;        LineStartTime_p = globalClock.getTime();&amp;#10;    }&amp;#10;    polygon_vertex_p.push(mouse_p.getPos());&amp;#10;    polygon_p.vertices = polygon_vertex_p;&amp;#10;} else {&amp;#10;    if (Drawing_p) {&amp;#10;        Drawing_p = false;&amp;#10;        LineLengths_p.push(polygon_p.vertices.length);&amp;#10;        LineDurations_p.push((globalClock.getTime() - LineStartTime_p));&amp;#10;        indx_shape_p += 1;&amp;#10;        Actionsincldel_p += 1;&amp;#10;        ActionsNotincldel_p += 1;&amp;#10;    }&amp;#10;}&amp;#10;if (((mouse_p.isPressedIn(UndoButton) &amp;&amp; FirstUndoClick_p) &amp;&amp; (indx_shape_p &gt; 0))) {&amp;#10;    UndoButton.fillColor = &quot;white&quot;;&amp;#10;    indx_shape_p -= 1;&amp;#10;    polygon_p = polygon_list_p[indx_shape_p];&amp;#10;    polygon_p.vertices = [[0, 0]];&amp;#10;    FirstUndoClick_p = false;&amp;#10;    NoDels_p += 1;&amp;#10;    DelRTs_p.push((globalClock.getTime() - startTime_p));&amp;#10;    ActionsNotincldel_p -= 1;&amp;#10;} else {&amp;#10;    if ((! mouse_p.isPressedIn(UndoButton))) {&amp;#10;        UndoButton.fillColor = &quot;darkgray&quot;;&amp;#10;        FirstUndoClick_p = true;&amp;#10;    }&amp;#10;}&amp;#10;face_p.draw();&amp;#10;for (var i_pol, _pj_c = 0, _pj_a = util.range((indx_shape_p + 1)), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    i_pol = _pj_a[_pj_c];&amp;#10;    polygon_list_p[i_pol].setAutoDraw(true);&amp;#10;    polygon_list_p[i_pol].draw();&amp;#10;}&amp;#10;&amp;#10;if (((mouse_p.isPressedIn(submit_button_p) &amp;&amp; Exit_p))) {&amp;#10;    submit_button_p.fillColor = &quot;white&quot;;&amp;#10;    Exit_p = false;&amp;#10;} else {&amp;#10;    if ((! mouse_p.isPressedIn(submit_button_p))) {&amp;#10;        submit_button_p.fillColor = &quot;darkgray&quot;;&amp;#10;        Exit_p = true;&amp;#10;    }&amp;#10;}" valType="extendedCode" updates="constant" name="Each JS Frame"/>
        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="var addname, NoCircs&amp;#10;&amp;#10;AllPolygonVertices_p=[]&amp;#10;&amp;#10;psychoJS.experiment.addData(&quot;ActionsNotincldel&quot;, ActionsNotincldel_p)&amp;#10;psychoJS.experiment.addData(&quot;Actionsincldel&quot;, Actionsincldel_p)&amp;#10;psychoJS.experiment.addData(&quot;NoDels&quot;, NoDels_p)&amp;#10;psychoJS.experiment.addData(&quot;DrawRTs&quot;, DrawRTs_p)&amp;#10;psychoJS.experiment.addData(&quot;DelRTs&quot;, DelRTs_p)&amp;#10;psychoJS.experiment.addData(&quot;LineLengths&quot;, LineLengths_p)&amp;#10;psychoJS.experiment.addData(&quot;LineDurations&quot;, LineDurations_p)&amp;#10;    &amp;#10;for (var i_pol = 0, _pj_a = indx_shape_p; (i_pol &lt; _pj_a); i_pol += 1) {&amp;#10;    polygon_list_p[i_pol].setAutoDraw(false);&amp;#10;    AllPolygonVertices_p.push(polygon_list_p[i_pol].vertices);&amp;#10;    //addname = (&quot;Line&quot; + i_pol.toString());&amp;#10;    //psychoJS.experiment.addData(addname, polygon_list[i_pol].vertices)&amp;#10;}&amp;#10;&amp;#10;for (var i_pol = 0, _pj_a = (CircleListList_p.length); (i_pol &lt; _pj_a); i_pol += 1) {&amp;#10;    NoCircs_p=CircleListList_p[i_pol].length;&amp;#10;    for (var i_pol2 = 0, _pj_a2 = NoCircs_p; (i_pol2 &lt; _pj_a2); i_pol2 += 1) {&amp;#10;    CircleListList_p[i_pol][i_pol2].setAutoDraw(false);&amp;#10;    }&amp;#10;}&amp;#10;&amp;#10;&amp;#10;psychoJS.experiment.addData(&quot;Circles&quot;, CircleCordsList_p);&amp;#10;psychoJS.experiment.addData(&quot;Lines&quot;, AllPolygonVertices_p)&amp;#10;//clear all circles&amp;#10;CircleList_p=[];&amp;#10;CircleListList_p=[];&amp;#10;//psychoJS.window.color = (0, 0, 0)&amp;#10;//psychoJS.window.clearBuffer({&quot;color&quot;: true})&amp;#10;//psychoJS.window.depthMask = true&amp;#10;//psychoJS.window.clearBuffer({&quot;color&quot;: false, &quot;depth&quot;: true, &quot;stencil&quot;: false});&amp;#10;//psychoJS.window.flip({&quot;clearBuffer&quot;: true});&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
